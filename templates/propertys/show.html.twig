{% extends 'base.html.twig' %}

{% block title %}Propertys{% endblock %}

{% block body %}
    <h1>Propertys</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ property.id }}</td>
            </tr>
            <tr>
                <th>Civilite</th>
                <td>{{ property.civilite }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ property.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ property.prenom }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ property.telephone }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ property.email }}</td>
            </tr>
            <tr>
                <th>Nreference</th>
                <td>{{ property.nreference }}</td>
            </tr>
            <tr>
                <th>Surface</th>
                <td>{{ property.surface }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ property.description }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ property.prix }}</td>
            </tr>
            <tr>
                <th>Chambres</th>
                <td>{{ property.chambres }}</td>
            </tr>
            <tr>
                <th>Salle_bains</th>
                <td>{{ property.salleBains }}</td>
            </tr>
            <tr>
                <th>Etages</th>
                <td>{{ property.etages }}</td>
            </tr>
            <tr>
                <th>Numero_etage</th>
                <td>{{ property.numeroEtage }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ property.adresse }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ property.ville }}</td>
            </tr>
            <tr>
                <th>Code_postale</th>
                <td>{{ property.codePostale }}</td>
            </tr>
            <tr>
                <th>Region</th>
                <td>{{ property.region }}</td>
            </tr>
            <tr>
                <th>Internet</th>
                <td>{{ property.internet ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Balcon</th>
                <td>{{ property.balcon ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Garage</th>
                <td>{{ property.garage ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Salle_sport</th>
                <td>{{ property.salleSport ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Piscine</th>
                <td>{{ property.piscine ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Camera_surveillance</th>
                <td>{{ property.cameraSurveillance ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>ImageName</th>
                <td>{{ property.imageName }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ property.createdAt ? property.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ property.updatedAt ? property.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_propertys_index') }}">back to list</a>

    <a href="{{ path('app_propertys_edit', {'id': property.id}) }}">edit</a>

    {{ include('propertys/_delete_form.html.twig') }}
{% endblock %}
